        "regex": "sumLatestVer = ([\\d]+);"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-$version-64-install.exe"
            },
            "32bit": {
                "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-$version-install.exe"
            }
        }
    },
    "installer": {
        "script": [
            "Push-Location \"$dir\"",
            "&\"$dir\\$fname\" -x",
            "Start-Sleep -Seconds 1",
            "Pop-Location"
        ]
    },
    "post_install": [
        "Remove-Item \"$dir\\$fname\"",
        "Write-Output '[Policies]' 'CopySelection    = 1' 'DiskAccess       = 1' 'FullscreenAccess = 1' 'InternetAccess   = 0' 'LinkProtocols    = http,https' 'PrinterAccess    = 0' 'RegistryAccess   = 0' 'SafeFileTypes    =' 'SavePreferences  = 1' | Out-File \"$dir\\sumatrapdfrestrict.ini\""
    ]
