{
    "homepage": "https://www.princexml.com/",
    "version": "16.2",
    "description": "Convert HTML to PDF. Beautiful printing with simple CSS.",
    "license": "Proprietary",
    "checkver": {
        "url": "https://www.princexml.com/download/",
        "re": ">Prince (\\d+\\.\\d+)<"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.princexml.com/download/prince-16.2-win64.msi#/prince.msi_",
            "hash": "e585f79b9095ef21f34765942be94aaf4f27c47a7fe134933423e279039166a5"
        },
        "32bit": {
            "url": "https://www.princexml.com/download/prince-16.2-win32.msi#/prince.msi_",
            "hash": "061687f3d9a2f0f51a12539c8a538aea2fbfb73b3086f16f0bef3983d8cdf5e4"
        }
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://www.princexml.com/download/prince-$version-win64.msi#/prince.msi_"
            },
            "32bit": {
                "url": "https://www.princexml.com/download/prince-$version-win32.msi#/prince.msi_"
            }
        }
    },
    "installer": {
        "script": [
            "Move-Item -Path \"$dir\\prince.msi_\" -Destination \"$dir\\prince.msi\" > $null",
            "Push-Location \"$dir\"",
            "lessmsi x \"$dir\\prince.msi\" > $null",
            "Pop-Location",
            "Move-Item -Path \"$dir\\prince\\SourceDir\\Prince\\*.*\" -Destination \"$dir\" > $null",
            "Move-Item -Path \"$dir\\prince\\SourceDir\\Prince\\engine\" -Destination \"$dir\" > $null",
            "Remove-Item \"$dir\\prince.msi\" > $null",
            "Remove-Item \"$dir\\prince\" -Recurse > $null"
        ]
    },
    "bin": "engine\\bin\\prince.exe",
    "shortcuts": [
        [
            "prince-gui.exe",
            "Prince XML"
        ]
    ]
}
